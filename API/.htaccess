RewriteRule    ^events/([0-9a-zA-Z]+)/?$    index.php?type=event&index=$1    [NC,L]


RewriteRule    ^users/*$    index.php?type=users [NC,L]
RewriteRule    ^users/([0-9a-zA-Z]+)/?$    index.php?type=user&index=$1    [NC,L]
RewriteRule    ^users/([0-9a-zA-Z]+)*/([0-9a-zA-Z]+)*$   index.php?type=userEvent&index=$2 [NC,L]
RewriteRule    ^users/([0-9a-zA-Z]+)*/([0-9a-zA-Z]+).([0-9a-zA-Z]+).([0-9a-zA-Z]+).([0-9a-zA-Z]+).([0-9a-zA-Z]+).([0-9a-zA-Z]+)$   index.php?type=userEventAdd&header=$2&description=$3&startDateTime=$4&endDateTime=$5&location=$6&owner=$7 [NC,L]

